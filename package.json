{
  "name": "tile-painter",
  "productName": "Tile Painter",
  "version": "0.0.0",
  "description": "Configure pinned desktop shortcuts in the Start menu for Windows.",
  "license": "MIT",
  "repository": "seangenabe/tile-painter",
  "author": {
    "name": "Sean Genabe",
    "email": "seangenabe@outlook.com"
  },
  "scripts": {
    "start": "electron .",
    "stylus": "stylus -c -m app/index.styl",
    "stylus:watch": "npm run stylus -- --watch",
    "build:electron": "electron-packager . --out=dist --prune --overwrite --platform=win32 --arch=x64 --icon=app/icon.ico --asar",
    "watch": "npm run stylus:watch",
    "build": "npm run stylus && npm run build:electron",
    "nodemon": "nodemon --watch app --exec npm start",
    "lint": "eslint index.js app"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "blob-to-buffer": "^1.2.6",
    "bootstrap": "^3.3.6",
    "bump-file": "file:modules\\bump-file",
    "chroma-js": "^1.1.1",
    "command-join": "^1.1.1",
    "concat-stream": "^1.5.1",
    "edge": "^5.9.2",
    "edge-ps": "0.1.0-pre",
    "electron-debug": "^1.0.0",
    "electron-prebuilt-path": "^2.0.0",
    "elevator": "^2.2.3",
    "end-of-stream": "^1.1.0",
    "get-pixels": "^3.3.0",
    "get-rgba-palette": "^2.0.0",
    "globby": "^5.0.0",
    "hex-rgb": "^1.0.0",
    "lodash.flatten": "^4.2.0",
    "lodash.sortby": "^4.5.0",
    "ltx": "^2.4.2",
    "mime-sniffer": "0.0.3",
    "minimist": "^1.2.0",
    "muicss": "^0.6.5",
    "mz": "^2.6.0",
    "nan": "^2.3.5",
    "purecss": "^0.6.0",
    "rgb-hex": "^1.0.0",
    "stylus": "^0.54.5",
    "temp": "^0.8.3",
    "tempy": "^0.1.0",
    "throw-down": "^0.1.7",
    "update-program-vem": "file:modules\\update-program-vem",
    "winreg": "^1.2.1",
    "xml": "^1.0.1",
    "xmldom": "^0.1.22",
    "yo-yo": "^1.2.2"
  },
  "devDependencies": {
    "chastifol": "^3.0.1",
    "devtron": "^1.1.0",
    "electron": "^1.6.10",
    "electron-packager": "^7.7.0",
    "eslint": "^4.0.0",
    "eslint-config-mirage": "^5.0.0",
    "nodemon": "^1.11.0"
  },
  "bundledDependencies": [
    "bump-file",
    "update-program-vem"
  ]
}
